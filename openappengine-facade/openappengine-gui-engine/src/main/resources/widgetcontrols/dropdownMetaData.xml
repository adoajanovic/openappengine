<?xml version="1.0" encoding="UTF-8"?>
<widget name="dropdown">
	<parameter name="id" mandatory="true"></parameter>
	<parameter name="labelId" mandatory="false"></parameter>
	<parameter name="path" mandatory="true"></parameter>
	<parameter name="name" mandatory="true"></parameter>
	<parameter name="required" mandatory="false" default="false"></parameter>
	<parameter name="value" mandatory="false"></parameter>
	<widget name="option">
		<parameter name="id" mandatory="true"></parameter>
		<parameter name="labelId" mandatory="true"></parameter>
		<parameter name="value" mandatory="true"></parameter>
	</widget>
</widget>