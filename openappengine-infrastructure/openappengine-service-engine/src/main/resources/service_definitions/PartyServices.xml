<?xml version="1.0" encoding="UTF-8"?>
<services>
	<service name="party.createPerson" class="com.openappengine.service.party.PartyServices" invokeMethod="createPerson">
		<description>
			Create Person.
		</description>
		<parameter name="person" type="com.openappengine.model.party.Person" mode="IN_OUT" optional="false" />
		<parameter name="partyContactMechs" type="java.util.List" mode="IN_OUT" optional="true" />
	</service>
	
	<service name="party.getActiveParties" class="com.openappengine.service.party.PartyServices" invokeMethod="getActiveParties">
		<description>
			Get All Active Parties.
		</description>
		<parameter name="personPartyList" type="java.util.List" mode="OUT" optional="false" />
	</service>
	
	<service name="party.findPersonById" class="com.openappengine.service.party.PartyServices" invokeMethod="findPersonById">
		<description>
			Find Parties By Id.
		</description>
		<parameter name="personId" type="java.lang.Integer" mode="IN" optional="false" />
		<parameter name="person" type="com.openappengine.model.party.Person" mode="OUT" optional="false" />
	</service>
	
	<service name="party.updatePerson" class="com.openappengine.service.party.PartyServices" invokeMethod="updatePerson">
		<description>
			Update Person Party
		</description>
		<parameter name="person" type="com.openappengine.model.party.Person" mode="IN_OUT" optional="false" />
		<parameter name="partyContactMechs" type="java.util.List" mode="IN_OUT" optional="true" />
	</service>
</services>