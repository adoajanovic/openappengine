<!-- 
	Builds a Linear Application WorkFlow using the Xml
-->
<appflow id="createOrderFlow">
	<step id="registerUser" sequence="1" stepHandler="com.ms.appflow.tests.TestStep1Handler" firstStep="true">
		<params>
			<param type="in" name="name" class="java.lang.String"></param>
			<param type="out" name="accountNo" class="java.lang.String"></param>
		</params>
	</step>
	<step id="registerUser2" sequence="2" stepHandler="com.ms.appflow.tests.TestStep2Handler">
		<params>
			<param type="in" name="accountNo" class="java.lang.String"></param>
		</params>
	</step>
</appflow>