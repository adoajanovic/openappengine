<?xml version="1.0" encoding="UTF-8"?>
<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.openappengine.com/EntityDefinition">
	<!-- Code Type -->
	<entity name="CodeType" class="com.openappengine.model.code.CodeType" is-deleteable="true">
		<field name="codeTypeId" type="Integer" property="codeTypeId" is-pk="true" is-autoincrement="true" is-required="true" is-updatable="false" is-numeric="true">
		</field>
		<field name="codeTypeValue" type="String" property="codeTypeValue" is-required="true" is-updatable="true"> 
		</field>
	</entity>
	
	<!-- Code Type -->
	<entity name="User_Registration">
		<field name="username" type="String" is-pk="true" is-required="true">
		</field>
		<field name="password" type="Password" is-pk="true" is-required="true">
		</field>
		<field name="firstName" type="String" is-required="true">
		</field>
		<field name="lastName" type="String" is-required="true">
		</field>
		<field name="comments" type="String" is-required="false" length="500">
		</field>
		<field name="date" type="Date" is-required="false">
		</field>
		<field name="Active" type="Boolean" is-required="false">
		</field>
		<field name="Salary" type="Boolean" is-required="false">
			<option value = ">100" />
			<option value = ">200" />
		</field>
	</entity>
</entities>