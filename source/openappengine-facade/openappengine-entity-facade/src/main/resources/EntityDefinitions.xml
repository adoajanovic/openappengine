<?xml version="1.0" encoding="UTF-8"?>
<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.openappengine.com/EntityDefinition">
	
	<!-- Code Type -->
	<entity name="CodeType" class="com.openappengine.model.code.CodeType" is-deleteable="true">
		<field name="codeTypeId" property="codeTypeId" is-pk="true" is-autoincrement="true" is-required="true" is-updatable="false" >
			<field-type type="text-field" />
		</field>
		<field name="codeTypeValue" property="codeTypeValue" is-required="true" is-updatable="true" >
			<field-type type="text-area" length="50"/>
		</field>
	</entity>
</entities>