<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<import resource="service-context.xml"/>
	
	<import resource="repository-context.xml"/>
	
	<bean name="facadeContext" class="com.openappengine.facade.context.FacadeContext">
	</bean>
	
	<bean name="partyManagerFacade" class="com.openappengine.facade.party.impl.PartyManagerFacadeImpl">
		<property name="partyManagerService" ref="partyManagerService"></property>
		<property name="codeRepository" ref="codeRepository"></property>
	</bean>
	
	<bean name="applicationDictionaryFacade" class="com.openappengine.facade.ad.ApplicationDictionaryFacadeImpl">
		<property name="applicationDictionaryService" ref="applicationDictionaryService"></property>
	</bean>
	
</beans>
