<?xml version="1.0" encoding="UTF-8"?>
<services>
	<service name="fms.addFleetVehicleType" class="com.openappengine.service.fms.FleetVehicleService" invokeMethod="addFleetVehicleType">
		<description>
			Add new Fleet Vehicle Type.
		</description>
		<parameter name="fleetVehicleType" type="com.openappengine.model.fms.FleetVehicleType" mode="IN_OUT" optional="false" />
	</service>
	
	<service name="fms.updateFleetVehicleType" class="com.openappengine.service.fms.FleetVehicleService" invokeMethod="updateFleetVehicleInfo">
		<description>
			Update Fleet Vehicle Type.
		</description>
		<parameter name="fleetVehicleType" type="com.openappengine.model.fms.FleetVehicleType" mode="IN_OUT" optional="false" />
	</service>
	
	<service name="fms.addFleetVehicle" class="com.openappengine.service.fms.FleetVehicleService" invokeMethod="addFleetVehicle">
		<description>
			Add a Fleet Vehicle.
		</description>
		<parameter name="fleetVehicle" type="com.openappengine.model.fms.FleetVehicle" mode="IN_OUT" optional="false" />
	</service>
	
</services>