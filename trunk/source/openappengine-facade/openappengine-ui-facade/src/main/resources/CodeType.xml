<?xml version="1.0" encoding="UTF-8"?>
<screen>
	
	<parameter>
		<param name="codeTypeId" value-ref="codeTypeId" required="true"/>
	</parameter>
	
	<pre-actions>
		<pre-action>
			<entity-find-one entity-name="CodeType" value-field="codeTypeValue" auto-field-map="true" condition="codeTypeId >= 1">
			</entity-find-one>
		</pre-action>
	</pre-actions>
	
	<container-panel>
		<center-panel id="ContainerPanel_Center">
			<widgets>
				<form name="CodeType" entity-value-ref="codeTypeValue">
					<entity-ref entity-name="CodeType" include-fields="auto" />
					<field-layout>
						<columns>2</columns>
					</field-layout>
					<render-mode>READ_WRITE</render-mode>
					<form-type>SIMPLE</form-type>
				</form>
			</widgets>
		</center-panel>
	</container-panel>
</screen>