	<queryString>
		<![CDATA[SELECT
tbl_subscriber.title,
tbl_subscriber.name,
tbl_address.flat_nr,
tbl_address.floor_nr,
ref_building.bldg_nm,
ref_area.area_nm,
tbl_address.city,
tbl_address.zip,
tbl_sales_invoice.invoice_id,
tbl_sales_invoice.invoice_nr,
tbl_sales_invoice.issue_dt,
tbl_sales_invoice.due_dt,
tbl_sales_invoice.invoice_period_start_dt,
tbl_sales_invoice.invoice_period_end_dt,
tbl_subscriber.account_nr,
'Active' as contract_status_desc,
(select SUM(mi.installment_amt) from tbl_monthly_installments mi where mi.invoice_id = tbl_sales_invoice.invoice_id) as installment_amt,
tbl_sales_invoice.payment_amt,45.0 as entertainment_tax,
tbl_sales_invoice.prev_balance,
tbl_sales_invoice.prev_payment,
tbl_sales_invoice.adjustments,
(select ifnull(sum(tbl_payment.amount),0.0) from tbl_payment where (tbl_payment.invoice_id = tbl_sales_invoice.invoice_id)) AS payment_amt_complete,
tbl_sales_invoice.total_amt,
82 as FTA, 'Rajesh Cable Networks' as company_desc
FROM tbl_subscriber, tbl_address, ref_area, ref_building, tbl_sales_invoice
where
tbl_subscriber.address_id = tbl_address.address_id AND
tbl_address.area_id = ref_area.area_id AND
tbl_address.bldg_id = ref_building.bldg_id AND
tbl_subscriber.subs_id = tbl_sales_invoice.subs_id AND
tbl_sales_invoice.status_id IN (select status_id from ref_sales_invoice_status where status_desc = 'Active') AND
tbl_subscriber.is_active = 'Y' AND
tbl_sales_invoice.invoice_period_start_dt = '2012-05-01 00:00:00' AND
tbl_sales_invoice.invoice_period_end_dt = '2012-05-31 00:00:00']]>
	</queryString>
