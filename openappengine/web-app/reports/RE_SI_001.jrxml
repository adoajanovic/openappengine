	<queryString>
		<![CDATA[SELECT
     prty.`pm_external_id`,
     per.`pm_first_name`,
     per.`pm_last_name`,
     oh.`oh_external_id`,
     oh.`oh_order_date`,
     oh.`oh_from_date`,
     oh.`oh_to_date`,
     oh.`oh_grand_total`,
     cn.`ch_contract_number`,
     cn.`ch_ar_amount`,
     ab.ab_address_1,
     ab.ab_city
FROM
     `fm_order_header` oh,
     `cn_contract_header` cn,
     `pm_party` prty,
     `pm_person` per,
     `ab_address_book` ab
WHERE
     oh.oh_contract_number = cn.ch_contract_number
 AND cn.ch_party_external_id = prty.pm_external_id
 AND prty.pm_party_id = per.pm_party_id
 AND ab.ab_attn_name = prty.pm_external_id]]>
